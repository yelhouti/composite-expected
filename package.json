{
  "name": "compositekey",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "description": "Description for compositekey",
  "license": "UNLICENSED",
  "scripts": {
    "backend:doc:test": "./gradlew javadoc -x webpack",
    "backend:info": "./gradlew -v",
    "backend:nohttp:test": "./gradlew checkstyleNohttp -x webpack",
    "backend:unit:test": "./gradlew test integrationTest -x webpack -Dlogging.level.ROOT=OFF -Dlogging.level.org.zalando=OFF -Dlogging.level.tech.jhipster=OFF -Dlogging.level.com.mycompany.myapp=OFF -Dlogging.level.org.springframework=OFF -Dlogging.level.org.springframework.web=OFF -Dlogging.level.org.springframework.security=OFF",
    "build": "npm run webpack:prod",
    "ci:backend:test": "npm run backend:info && npm run backend:doc:test && npm run backend:nohttp:test && npm run backend:unit:test",
    "ci:e2e:package": "npm run java:$npm_package_config_packaging:$npm_package_config_default_environment -- -Pe2e -Denforcer.skip=true",
    "postci:e2e:package": "cp build/libs/*SNAPSHOT.$npm_package_config_packaging e2e.$npm_package_config_packaging",
    "ci:e2e:prepare": "npm run ci:e2e:prepare:docker",
    "ci:e2e:prepare:docker": "npm run docker:db:up && npm run docker:others:up && docker ps -a",
    "ci:e2e:run": "concurrently -k -s first \"npm run ci:e2e:server:start\" \"npm run e2e:headless\"",
    "preci:e2e:server:start": "npm run docker:db:await --if-present && npm run docker:others:await --if-present",
    "ci:e2e:server:start": "java -jar e2e.$npm_package_config_packaging --spring.profiles.active=$npm_package_config_default_environment -Dlogging.level.ROOT=OFF -Dlogging.level.org.zalando=OFF -Dlogging.level.tech.jhipster=OFF -Dlogging.level.com.mycompany.myapp=OFF --logging.level.org.springframework.web=ERROR",
    "ci:e2e:teardown": "npm run ci:e2e:teardown:docker",
    "ci:e2e:teardown:docker": "npm run docker:db:down --if-present && npm run docker:others:down && docker ps -a",
    "ci:frontend:build": "npm run webpack:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "ci:server:await": "echo \"Waiting for server at port $npm_package_config_backend_port to start\" && wait-on http-get://localhost:$npm_package_config_backend_port/management/health && echo \"Server at port $npm_package_config_backend_port started\"",
    "ci:server:package": "npm run java:$npm_package_config_packaging:$npm_package_config_default_environment",
    "clean-www": "rimraf build/resources/main/static/app/{src,build/}",
    "cleanup": "rimraf build/resources/main/static/",
    "docker:db:down": "docker-compose -f src/main/docker/postgresql.yml down -v --remove-orphans",
    "docker:db:up": "docker-compose -f src/main/docker/postgresql.yml up -d",
    "docker:others:down": "",
    "predocker:others:up": "",
    "docker:others:up": "",
    "e2e": "npm run e2e:protractor",
    "e2e:dev": "concurrently -k -s first \"./mvnw\" \"e2e:run\"",
    "pree2e:headless": "npm run ci:server:await",
    "e2e:headless": "npm run e2e:protractor:headless",
    "e2e:protractor": "protractor src/test/javascript/protractor.conf.js",
    "e2e:protractor:headless": "npm run e2e:protractor",
    "e2e:update-webdriver": "webdriver-manager update --gecko false",
    "postinstall": "npm run e2e:update-webdriver",
    "java:docker": "./gradlew bootJar jibDockerBuild",
    "java:docker:dev": "npm run java:docker -- -Pdev,webpack",
    "java:docker:prod": "npm run java:docker -- -Pprod",
    "java:jar": "./gradlew bootJar -x test -x integrationTest",
    "java:jar:dev": "npm run java:jar -- -Pdev,webpack",
    "java:jar:prod": "npm run java:jar -- -Pprod",
    "java:war": "./gradlew bootWar -Pwar -x test -x integrationTest",
    "java:war:dev": "npm run java:war -- -Pdev,webpack",
    "java:war:prod": "npm run java:war -- -Pprod",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config src/test/javascript/jest.conf.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,json,yml,js,ts,tsx,css,scss,java}\"",
    "serve": "npm run start",
    "start": "npm run webpack:dev",
    "start-tls": "npm run webpack:dev -- --env.tls",
    "pretest": "npm run lint",
    "test": "ng test --coverage --logHeapUsage -w=2",
    "test:watch": "npm run test -- --watch",
    "webpack:build": "npm run cleanup && npm run webpack:build:dev",
    "webpack:build:dev": "ng build",
    "webpack:build:prod": "ng build --prod",
    "webpack:dev": "ng serve",
    "webpack:dev-verbose": "ng serve --verbose",
    "webpack:prod": "npm run cleanup && npm run webpack:build:prod && npm run clean-www",
    "webpack:test": "npm run test"
  },
  "config": {
    "backend_port": "8080",
    "default_environment": "prod",
    "packaging": "jar"
  },
  "dependencies": {
    "@angular/common": "10.2.3",
    "@angular/compiler": "10.2.3",
    "@angular/core": "10.2.3",
    "@angular/forms": "10.2.3",
    "@angular/localize": "10.2.3",
    "@angular/platform-browser": "10.2.3",
    "@angular/platform-browser-dynamic": "10.2.3",
    "@angular/router": "10.2.3",
    "@fortawesome/angular-fontawesome": "0.7.0",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "bootstrap": "4.5.3",
    "dayjs": "1.9.5",
    "ng-jhipster": "0.16.0",
    "ngx-cookie-service": "10.1.1",
    "ngx-infinite-scroll": "10.0.0",
    "ngx-webstorage": "6.0.0",
    "rxjs": "6.6.3",
    "swagger-ui-dist": "3.36.2",
    "tslib": "2.0.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-builders/jest": "10.0.1",
    "@angular-eslint/eslint-plugin": "0.6.0-beta.0",
    "@angular/cli": "10.2.0",
    "@angular/compiler-cli": "10.2.3",
    "@angular/service-worker": "10.2.3",
    "@openapitools/openapi-generator-cli": "1.0.13-4.3.1",
    "@types/chai": "4.2.11",
    "@types/chai-string": "1.4.2",
    "@types/jest": "26.0.15",
    "@types/mocha": "7.0.2",
    "@types/node": "13.13.4",
    "@types/selenium-webdriver": "4.0.9",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "browser-sync": "2.26.13",
    "browser-sync-webpack-plugin": "2.2.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.5.0",
    "concurrently": "5.3.0",
    "copy-webpack-plugin": "6.3.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-loader": "4.0.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "generator-jhipster": "6.10.5",
    "husky": "4.2.5",
    "jest": "26.6.3",
    "jest-date-mock": "1.0.8",
    "jest-junit": "11.1.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "10.4.2",
    "merge-jsons-webpack-plugin": "1.0.21",
    "mocha": "8.0.1",
    "prettier": "2.1.2",
    "prettier-plugin-java": "0.8.3",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "simple-progress-webpack-plugin": "1.1.2",
    "ts-node": "8.10.2",
    "typescript": "4.0.5",
    "wait-on": "5.2.0",
    "webdriver-manager": "12.1.7",
    "webpack-bundle-analyzer": "4.1.0",
    "webpack-notifier": "1.8.0"
  },
  "engines": {
    "node": ">=12.18.3"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "build/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
